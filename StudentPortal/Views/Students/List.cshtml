@{
}

@model List<StudentPortal.Models.Entities.Student>
<h1>Student</h1>

<table class="table">
    <thead>                     x
    <tr>
       <th>ID</th>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>City</th>
    </tr>

    </thead>
    <tbody>
        @foreach(var Student in Model)
        {
            <tr>
                <td>@Student.Id</td>
                <td>@Student.Name</td>
                <td>@Student.email</td>
                <td>@Student.phone</td>
                <td>@Student.city</td>
                <td>

                    <a class="btn btn-primary" asp-controller ="Students" asp-action="edit" asp-route-id="@Student.Id">Edit</a>
                </td>
            </tr>
        }






    </tbody>
</table>